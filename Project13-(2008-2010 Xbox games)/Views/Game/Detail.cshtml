@model Project13__2008_2010_Xbox_games_.Models.Game
@{
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
    ViewBag.Title = "2004-2010 Xbox Games";

}


<div>
    <img src="~/Images/@Model.CoverImagerFileName" />
    <h4>@Model.Title</h4>
    <p>@Model.DateOfRelease.ToShortDateString()</p>
    <p>@Model.Developer</p>
    <p>@Model.Genre</p>
    <p>@Model.Description</p>

</div>

